	<xs:import namespace="http://cybox.mitre.org/common-2" schemaLocation="../cybox_common.xsd"/>
	<xs:import namespace="http://cybox.mitre.org/objects#HTTPSessionObject-2" schemaLocation="HTTP_Session_Object.xsd"/>
