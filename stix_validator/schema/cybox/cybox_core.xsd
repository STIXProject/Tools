					<xs:documentation>The Observable construct represents a description of a single cyber observable.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="operator" type="cybox:OperatorTypeEnum" use="required">
